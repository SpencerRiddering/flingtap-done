    <string name="help_task_List">Task List / Help</string>	
    <string name="help_task_list_fast_add_box">Fast-add text box. Type a task title into this box and press the enter key (or tap the "Add" button) to quickly add a new task. </string>	
    <string name="help_task_list_fast_add_button">Fast-add add button. When this button is tapped any text in the fast-add text box will be used to create a new task.</string>	
    <string name="help_task_list_fast_add_speech_recognition_button">Speech recognition button. Prompts user to say the name of a task and then converts their spoken words to text.</string>	
    <string name="help_task_list_task_priority_indicator">Priority speedometer. Displays the task\'s priority.</string>	
    <string name="help_task_list_task_title">Task title. Tapping this area will open the task in the editor. Long press to view context menu. See #17, #18 and #19 below for more details.</string>	
    <string name="help_task_list_task_title_all">Task title. Tapping this area will open the task in the editor. Long press to view context menu. See #17, #18, #19, #20 and #21 below for more details.</string>	
    <string name="help_task_list_task_due_date">Task due date.</string>	
    <string name="help_task_list_options_menu_new_task">New task menu item. Creates a new task and opens it in the editor.</string>
    <string name="help_task_list_options_menu_filter">Filter menu item. Opens a screen for filtering your tasks.</string>	
    <string name="help_task_list_options_menu_delete_completed">Delete completed tasks menu item. Deletes all completed tasks.</string> <!-- Note: some completed tasks may not be displayed because of the filter settings. -->	
    <string name="help_task_list_options_menu_archive_completed">Archive completed tasks menu item. Archives all completed tasks.</string>	
    <string name="help_task_list_options_menu_settings">Settings menu item. Opens the settings configuration screen. Here you can enable and disable application features and generally configure the application\'s behavior.</string>	
    <string name="help_task_list_options_menu_more">More menu item. Displays additional menu items. See #12 through #16 below.</string>	
    <string name="help_task_list_options_menu_about">About menu item. Displays details about this application.</string>	
    <string name="help_task_list_options_menu_feedback">Feedback menu item. Opens a our feedback website where you can suggest and vote for new features.</string>	
    
    <string name="help_task_list_ctx_menu_mark_complete">Mark task complete context menu item. Marks the task complete (or pending if the task is already complete).</string>	
    <string name="help_task_list_ctx_menu_edit_task">Edit task context menu item. Opens the selected task in the task editor.</string>	
    <string name="help_task_list_ctx_menu_delete_task">Delete task context menu item. Deletes the selected task.</string>	
    <string name="help_task_list_ctx_menu_archive_task">Archive task context menu item (optional). Moves the selected task to the archive.</string>	
    <string name="help_task_list_ctx_menu_label_task">Label task context menu item (optional). Opens a dialog containing a list of existing labels that can be applied to the selected task.</string>	

